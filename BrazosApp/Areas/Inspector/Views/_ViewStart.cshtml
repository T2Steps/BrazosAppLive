@using System.Security.Claims;
@using BrazosApp.Utility;
@{
      Layout = "_LoginLayOut";
      ViewBag.baseurl = config.GetSection("BaseUrl").Value;

      ViewBag.role = User.FindFirstValue(ClaimTypes.Role);

      ViewBag.userId = User.FindFirstValue(ClaimTypes.NameIdentifier);

      //ViewBag.EncryptedUiD = User.FindFirstValue("EncryptedUiD");

      //ViewBag.userName = User.FindFirstValue(ClaimTypes.Name);
      ViewBag.randomNumber = (new Random()).Next(1, 999999).ToString();

      ViewBag.Dashboard = "/Inspector/Dashboard";
      ViewBag.apiUrl = config.GetSection("APIUrl").Value;
}


