<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 style="color:#A48464">Solicitud de permiso	 </h1>
            </div>
            <div class="col-sm-3">
            </div>
            <div class="col-sm-3">
                <input type="text" name="AppDt" id="Appdt" class="form-control shadow" value="Fecha de aplicacion: @DateTime.Now.Date.ToShortDateString()" style="font-weight:bold; font-size:15px" disabled />
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<section class="content">
    <div class="row">
        <div class="col-12">
            <div class="card card-outline " style="border-top:3px solid #022E5F">

                <div class="card-body">

                    <form action="/RFNewPermit" enctype="multipart/form-data" method="post" id="NewPermitDataForm">
                        <fieldset id="info">
                            <div class="row shadow mb-1" style="border:1px solid lightgrey; background-color:whitesmoke">
                                <h5 class="pt-1" style="color:#A48464">DETALLES DEL ESTABLECIMIENTO</h5>
                            </div>
                            <input name="ApplicationForId" id="applicationforId" value="@Model.ApplicationForId" hidden />
                            <input name="ApplicationId" id="applicationId" value="@Model.ApplicationId" hidden />
                            <input name="Establishment.Id" id="estId" value="0" hidden />
                            <div class="row">
                                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Nombre del establecimiento <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" name="Establishment.Name" id="EstName" class="form-control shadow mb-2 required" placeholder="Nombre del establecimiento" />
                                    <span id="EstNameError" class="error text-danger"></span>
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Calle <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Establishment.Address" id="strt" placeholder="Calle" />
                                    <span id="strtError" class="error text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Cuidad <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Establishment.City" id="cty" placeholder="Cuidad" />
                                    <span id="ctyError" class="error text-danger"></span>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Estado <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Establishment.State" id="state" placeholder="Estado" value="TX" />
                                    <span id="stateError" class="error text-danger"></span>
                                </div>
                                
                                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Código postal <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Establishment.Zip" id="zip" placeholder="Código postal" />
                                    <span id="zipError" class="error text-danger"></span>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Numero de teléfono <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Establishment.ContactNo" id="contactNo" placeholder="Numero de teléfono" />
                                    <span id="contactError" class="error text-danger"></span>
                                </div>
                            </div>
                            <div class="row mt-5 shadow  mb-1" style="border:1px solid lightgrey; background-color:whitesmoke">
                                @*<h5 class="pt-1" style="color:#A48464">DETALLES DEL PROPIETARIO</h5>*@
                                <div class="row pt-1">
                                    <div class="col-lg-7 col-md-7 col-sm-12 col-xs-12">
                                          <h5 style="color:#A48464">DETALLES DEL PROPIETARIO</h5>
                                    </div>
                                    <div class="col-md-5 col-lg-5 col-sm-12 col-xs-12">
                                          <div class="icheck-marine">
                                                <input type="checkbox" id="sameAddresscheck" onclick="if($(this).is(':checked')){$('#oAddress').val($('#strt').val());$('#ocity').val($('#cty').val());$('#ostate').val($('#state').val());$('#ozip').val($('#zip').val());$('#oAddress').trigger('change');$('#ocity').trigger('change');$('#ostate').trigger('change');$('#ozip').trigger('change'); } else{$('#oAddress').val('');$('#ocity').val('');$('#ostate').val('');$('#ozip').val('');}">
                                                <label  for="sameAddresscheck">
                                                      Lo mismo que la dirección del establecimiento
                                                </label>
                                          </div>
                                          @*<input type="checkbox" id="sameAddresscheck"  />
                                          <span style="font-weight:bold">Same As Establishment Address</span>*@
                                    </div>
                                 </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-6 col-md-3 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Dueño <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" name="Owner.Name" id="OwnName" class="form-control shadow mb-2 required" placeholder="Dueño" />
                                    <span id="OwnNameError" class="error text-danger"></span>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Correo electrónico <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Owner.EmailId" id="emailAddr" placeholder="Correo electrónico" />
                                    <span id="emailAddrError" class="error text-danger"></span>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Numero de teléfono <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Owner.ContactNo" id="ocontactNo" placeholder="Numero de teléfono" />
                                    <span id="ocontactNoError" class="error text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Domicilio de envió <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Owner.MailingAddress" id="oAddress" placeholder="Domicilio de envió" />
                                    <span id="oAddressError" class="error text-danger"></span>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Cuidad <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Owner.City" id="ocity" placeholder="Cuidad" />
                                    <span id="ocityError" class="error text-danger"></span>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Estado <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Owner.State" id="ostate" placeholder="Estado" value="TX" />
                                    <span id="ostateError" class="error text-danger"></span>
                                </div>
                                
                                <div class="col-lg-2 col-md-2 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Código postal <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <input type="text" class="form-control shadow mb-2 required" name="Owner.Zip" id="ozip" placeholder="Código postal" />
                                    <span id="ozipError" class="error text-danger"></span>
                                </div>

                            </div>
                            <div class="row mt-5 shadow  mb-1" style="border:1px solid lightgrey; background-color:whitesmoke">
                                <h5 class="pt-1" style="color:#A48464">DETALLES DE FUNCIONAMIENTO</h5>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                    <input type="checkbox" name="OperationDetails.WithinCityLimitChoice" id="citylimit" value="true" hidden />
