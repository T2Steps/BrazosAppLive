@{
      var code = Model.Response!.Code;
      var apiUrl = ViewBag.apiUrl;
    var baseUrl = ViewBag.baseurl;
      var Dashboard = ViewBag.Dashboard.ToString();
      var Address = new string[4];
      var backurl = "";
      if (code == "RF")
      {
            ViewData["Title"] = "Retail Food Inspection";
            backurl = "/Schedule?code=RF";

      }
      else if (code == "MF")
      {
            ViewData["Title"] = "Mobile Food Inspection";
            backurl = "/Schedule?code=MF";
      }
      if (Model.Response.SId != 0)
      {
            Address = Model.Response!.Address!.Split(',');
      } 
}

