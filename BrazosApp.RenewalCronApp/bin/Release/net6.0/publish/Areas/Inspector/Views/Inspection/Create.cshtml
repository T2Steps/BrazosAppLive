    <script>
            var editingRow = null; // Track the row being edited

            // Function to handle Edit
            function Edit(button) {
                $('#tblCustomers').find("tr").css('background-color', '')

                // Find the row to edit
                var row = $(button).closest("TR");

                // Save the row being edited
                editingRow = row;

                // Get the values from the row
                var name = $("TD", row).eq(0).text();
                var country = $("TD", row).eq(1).text();

                // Populate the textboxes with the row values
                $("#txtName").val(name);
                $("#txtCountry").val(country);

                // Highlight the row to show it's being edited
                row.css('background-color', '#ffed85');

                // Change "Add" button to "Save" and bind the Save function
                $("#btnAdd").val("Save").off("click").on("click", SaveEdit);
                $('#btnCancel').show();
                $('#btnCancel').on("click", CancelEdit)
            }

            function CancelEdit(){
                $('#tblCustomers').find("tr").css('background-color', '')
                $("#txtName").val('');
                $("#txtCountry").val('');
                $('#btnCancel').hide();
                $("#btnAdd").val("Add").off("click").on("click", AddRow);
            }

            // Function to Save Edited Values
            function SaveEdit() {
                if (editingRow) {
                    // Get the new values from the textboxes
                    var newName = $("#txtName").val();
                    var newCountry = $("#txtCountry").val();

                    // Validate that the fields are not empty
                    if (newName === '' || newCountry === '') {
                        toastr.error("Please Fill Up Required Fields");
                        return;
                    }

                    // Update the table cells' visible text (without removing hidden inputs)
                    $("TD", editingRow).eq(0).contents().filter(function () {
                        return this.nodeType === 3; // This ensures only the text node is updated, not the hidden input
                    }).replaceWith(newName);

                    $("TD", editingRow).eq(1).contents().filter(function () {
                        return this.nodeType === 3;
                    }).replaceWith(newCountry);

                    // Update the hidden input values
                    $(editingRow).find("input#FinalTemperatureList_ItemName").val(newName);
                    $(editingRow).find("input#FinalTemperatureList_Temperature").val(newCountry);

                    // Clear the background highlight after saving
                    editingRow.css('background-color', '');

                    // Clear the input fields
                    $("#txtName").val("");
                    $("#txtCountry").val("");

                    $('#btnCancel').hide();
                    // Change "Save" button back to "Add" and restore the Add functionality
                    $("#btnAdd").val("Add").off("click").on("click", AddRow);

                    // Clear the editing row reference
                    editingRow = null;
                }
            }


            // Function to Add New Row
            function AddRow() {
                var validationFlg = 1;

                var txtName = $("#txtName");
                var txtCountry = $("#txtCountry");
                var txtCount = $("#txtCount");

                // Validation logic
                if (txtName.val() == '') {
                    txtName.css('border-color', 'red');
                    setTimeout(() => {
                        txtName.css('border-color', '');
                    }, 4000);
                    validationFlg = 0;
                }

                if (txtCountry.val() == '') {
                    txtCountry.css('border-color', 'red');
                    setTimeout(() => {
                        txtCountry.css('border-color', '');
                    }, 4000);
                    validationFlg = 0;
                }

                if (validationFlg == 0) {
                    $('audio#errorsound')[0].play();
                    setTimeout(() => {
                        toastr.error("Please Fill Up Required Fields");
                    }, 775);
                    return false;
                }

                // Get reference to the table body
                var tBody = $("#tblCustomers > TBODY")[0];
                var row = tBody.insertRow(-1);

                // Add Name and Hidden Input
                var cell = $(row.insertCell(-1));
                cell.html(txtName.val());
                cell.append("<input type='hidden' id='FinalTemperatureList_ItemName' name='FinalTemperatureList[" + txtCount.val() + "].ItemName' value='" + txtName.val() + "'>");

                // Add Country and Hidden Input
                cell = $(row.insertCell(-1));
                cell.html(txtCountry.val());
                cell.append("<input type='hidden' id='FinalTemperatureList_Temperature' name='FinalTemperatureList[" + txtCount.val() + "].Temperature' value='" + txtCountry.val() + "'>");

                // Increment the count for the next entry
                txtCount.val(parseInt(txtCount.val()) + 1);

                // Add Edit and Remove Buttons
                cell = $(row.insertCell(-1));

                // Add Edit Button
                var btnEdit = $("<input />");
                btnEdit.attr("type", "button");
                btnEdit.attr("onclick", "Edit(this);");
                btnEdit.addClass('btn btn-lg btn-primary edit valid');
                btnEdit.val("Edit");
                cell.append(btnEdit);

                // Add Remove Button
                var btnRemove = $("<input />");
                btnRemove.attr("type", "button");
                btnRemove.attr("onclick", "Remove(this);");
                btnRemove.addClass('btn btn-lg btn-danger remove valid');
                btnRemove.val("Remove");
                cell.append(btnRemove);

                // Clear the textboxes
                txtName.val("");
                txtCountry.val("");
            }

            // Initial event listener for Add button
            $("#btnAdd").on("click", AddRow);

            // Remove function (unchanged)
            function Remove(button) {
                var row = $(button).closest("TR");
                var name = $("TD", row).eq(0).html();
                $('audio#warning')[0].play();
                setTimeout(() => {
                    Swal.fire({
                        title: 'Are you sure?',
                        icon: 'warning',
                        showCancelButton: true,
                        confirmButtonColor: '#8B9BB2',
                        cancelButtonColor: '#d33',
                        confirmButtonText: 'Yes, delete it!'
                    }).then((result) => {
                        if (result.isConfirmed) {
                            var table = $("#tblCustomers")[0];
                            table.deleteRow(row[0].rowIndex);

                            var cnt = $("#txtCount").val();
                            $("#txtCount").val(parseInt(cnt) - 1);

                            $("#tblCustomers > TBODY > TR").each(function (index) {
                                var itemLocationInput = $(this).find("input[type='hidden'][id='FinalTemperatureList_ItemName']");
                                var temperatureInput = $(this).find("input[type='hidden'][id='FinalTemperatureList_Temperature']");

                                itemLocationInput.attr("name", "FinalTemperatureList[" + index + "].ItemName");
                                temperatureInput.attr("name", "FinalTemperatureList[" + index + "].Temperature");
                            });
                        }
                    });
                }, 100);
            }
