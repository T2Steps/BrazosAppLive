                                        </div>
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="choice" id="ctylimitChoice2" onchange="WithinCityLimit()">
                                            <label class="form-check-label btn btn-light shadow mb-3 px-3" for="inlineCheckbox2" style="font-size: 14px" onclick="$('#ctylimitChoice2').prop('checked', true); $('#ctylimitChoice2').trigger('change')">No</label>
                                        </div>
                                    </div>
                                    <span id="emailError" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Tipo de negocio <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <select class="custom-select rounded-1 shadow mb-3 required" id="businessId" name="OperationDetails.BusinessTypeId" asp-items="@Model.BusinessTypesSp">
                                        <option disabled selected>---Select---</option>
                                        @* <option value="1">Restaurante/producción de comida </option>
                                        <option value="2">Establecimiento con preparación de alimentos o bebidas TCS </option>
                                        <option value="3">Establecimiento sin preparación de alimentos o bebidas TCS </option>
                                        <option value="4">Bar con ingredientes TCS o alimentos </option> *@
                                    </select>
                                    <span id="businessIdError" class="error text-danger"></span>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Tipo de operación <span style="font-size: 20px ;color:red">&#42;</span></label>
                                    <select class="custom-select rounded-1 shadow mb-3 required" id="operationtypeId" name="OperationDetails.OperationTypeId" asp-items="@Model.OperationTypesSp">
                                        <option disabled selected>---Select---</option>
                                        @* <option value="1">Anual (12 meses)</option>
                                        <option value="2">Temporal (4 meses)</option> *@
                                    </select>
                                    <span id="operationtypeIdError" class="error text-danger"></span>
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 form-group">
                                    <label style="font-weight:600" class="col-form-label">Número de empleados actuales <span style="font-size: 20px ;color:red">&#42;</span></label>
