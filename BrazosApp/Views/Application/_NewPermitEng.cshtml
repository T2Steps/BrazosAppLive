        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 style="color:#A48464">Food Permit Application	</h1>
            </div>
            <div class="col-sm-3">
            </div>
            <div class="col-sm-3">
                <input type="text" name="AppDt" id="Appdt" class="form-control shadow" value="Application Date: @DateTime.Now.Date.ToShortDateString()" style="font-weight:bold" disabled />
            </div>
        </div>
    </div><!-- /.container-fluid -->
